{
	"$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
	"version": 2,
	"final_space": true,
	"console_title_template": "{{ .PWD }}",
	"palette": {
		"light-grey": "#666666",
		"black": "#030303",
		"blue": "#4B95E9",
		"green": "#59C9A5",
		"orange": "#F07623",
		"red": "#D81E5B",
		"white": "#EEEEEE",
		"yellow": "#F3AE35"
	},
	"transient_prompt": {
		"template": "<p:yellow,transparent>\ue0b6</><,p:yellow> {{ .Folder }} </><p:yellow,transparent>\ue0b0</> ",
		"background": "transparent",
		"foreground": "p:black",
		"Text": "",
		"Duration": 0,
		"NameLength": 0
	},
	"secondary_prompt": {
		"template": "<p:yellow,transparent>\ue0b6</><,p:yellow> > </><p:yellow,transparent>\ue0b0</> ",
		"background": "transparent",
		"foreground": "p:black",
		"Text": "",
		"Duration": 0,
		"NameLength": 0
	},
	"blocks": [
		{
			"type": "prompt",
			"alignment": "left",
			"segments": [
				{
					"type": "path",
					"style": "plain",
					"disable_cursor_positioning": true,
					"powerline_symbol": "\ue0b0",
					"template": " \uea83 {{ path .Path .Location }} ",
					"properties": {
						"style": "folder"
					},
					"background": "terminal_background",
					"foreground": "p:white",
					"Text": "",
					"Duration": 0,
					"NameLength": 0
				},
				{
					"type": "git",
					"style": "plain",
					"foreground_templates": [
						"{{ if or (.Working.Changed) (.Staging.Changed) }}p:black{{ end }}",
						"{{ if and (gt .Ahead 0) (gt .Behind 0) }}p:white{{ end }}",
						"{{ if gt .Ahead 0 }}p:white{{ end }}"
					],
					"template": " {{ if .UpstreamURL }}{{ url .UpstreamIcon .UpstreamURL }} {{ end }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }} ",
					"properties": {
						"branch_max_length": 25,
						"fetch_status": true,
						"fetch_upstream_icon": true
					},
					"foreground": "p:black",
					"Text": "",
					"Duration": 0,
					"NameLength": 0
				},
				{
					"type": "root",
					"style": "powerline",
					"powerline_symbol": "\ue0b0",
					"template": " \uf0e7 ",
					"background": "p:yellow",
					"foreground": "p:white",
					"Text": "",
					"Duration": 0,
					"NameLength": 0
				}
			]
		},
		{
			"type": "rprompt",
			"segments": [
				{
					"type": "node",
					"style": "plain",
					"template": "\ue718 ",
					"properties": {
						"display_mode": "files",
						"fetch_package_manager": false,
						"home_enabled": false
					},
					"background": "transparent",
					"foreground": "p:green",
					"Text": "",
					"Duration": 0,
					"NameLength": 0
				},
				{
					"type": "go",
					"style": "plain",
					"template": "\ue626 ",
					"properties": {
						"fetch_version": false
					},
					"background": "transparent",
					"foreground": "p:blue",
					"Text": "",
					"Duration": 0,
					"NameLength": 0
				},
				{
					"type": "python",
					"style": "plain",
					"template": "\ue235 ",
					"properties": {
						"display_mode": "files",
						"fetch_version": false,
						"fetch_virtual_env": false
					},
					"background": "transparent",
					"foreground": "p:yellow",
					"Text": "",
					"Duration": 0,
					"NameLength": 0
				},
				{
					"type": "shell",
					"style": "plain",
					"template": "in <p:blue><b>{{ .Name }}</b></> ",
					"background": "transparent",
					"foreground": "p:white",
					"Text": "",
					"Duration": 0,
					"NameLength": 0
				},
				{
					"type": "time",
					"style": "plain",
					"template": "at <p:blue><b>{{ .CurrentDate | date \"15:04:05\" }}</b></>",
					"background": "transparent",
					"foreground": "p:white",
					"Text": "",
					"Duration": 0,
					"NameLength": 0
				}
			]
		}
	],
	"tooltips": [
		{
			"type": "gcp",
			"tips": [
				"gcp"
			],
			"style": "powerline",
			"template": " îž² {{.Project}} :: {{.Account}}",
			"properties": {
				"display_default": true
			},
			"background": "transparent",
			"foreground": "#6666FF",
			"Text": "",
			"Duration": 0,
			"NameLength": 0
		}
	]
}
