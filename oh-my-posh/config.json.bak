{
	"$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
	"version": 2,
	"final_space": true,
	"disable_cursor_positioning": false,
	"console_title_template": "{{ .PWD }}",
	"palette": {
		"grey": "#AAAAAA",
		"light-grey": "#999999",
		"black": "#030303",
		"blue": "#4B95E9",
		"green": "#59C9A5",
		"orange": "#F07623",
		"red": "#D81E5B",
		"white": "#E0E0EA",
		"yellow": "#F3AE35"
	},
	"transient_prompt": {
		"template": "<p:yellow,transparent>\ue0b6</><,p:yellow> {{ .Folder }} </><p:yellow,transparent>\ue0b0</> ",
		"background": "transparent",
		"foreground": "p:black",
		"Text": "",
		"Duration": 0,
		"NameLength": 0
	},
	"secondary_prompt": {
		"template": "<p:yellow,transparent>\ue0b6</><,p:yellow> > </><p:yellow,transparent>\ue0b0</> ",
		"background": "transparent",
		"foreground": "p:black",
		"Text": "",
		"Duration": 0,
		"NameLength": 0
	},
	"blocks": [
		{
			"type": "prompt",
			"alignment": "left",
			"segments": [
				{
					"type": "path",
					"style": "plain",
					"template": "{{ .Path }}",
					"properties": {
						"style": "full",
						"mapped_locations": {
							"~/Work/regsoft/src": "\uf0e3 "
						}
					},
					"foreground": "p:white"
				},
				{
					"type": "root",
					"style": "plain",
					"template": " \uf0e7 ",
					"background": "p:yellow",
					"foreground": "p:white",
					"Text": "",
					"Duration": 0,
					"NameLength": 0
				}
			]
		},
		{
			"type": "rprompt",
			"segments": [
				{
					"type": "node",
					"style": "plain",
					"template": "\ue718 ",
					"properties": {
						"display_mode": "files",
						"fetch_package_manager": false,
						"home_enabled": false
					},
					"background": "transparent",
					"foreground": "p:green",
					"Text": "",
					"Duration": 0,
					"NameLength": 0
				},
				{
					"type": "git",
					"style": "plain",
					"foreground_templates": [
						"{{ if or (.Working.Changed) (.Staging.Changed) }}p:black{{ end }}",
						"{{ if and (gt .Ahead 0) (gt .Behind 0) }}p:grey{{ end }}",
						"{{ if gt .Ahead 0 }}p:light-grey{{ end }}"
					],
					"template": "{{ if .UpstreamURL }}{{ url .UpstreamIcon .UpstreamURL }}{{ end }}{{ .HEAD }} {{if .BranchStatus }}{{ .BranchStatus }}{{ end }} {{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }} ",
					"properties": {
						"branch_max_length": 25,
						"fetch_status": true,
						"fetch_upstream_icon": true
					},
					"Text": "",
					"Duration": 0,
					"NameLength": 0
				}
			]
		}
	],
	"tooltips": [
		{
			"type": "gcp",
			"tips": [
				"gcp"
			],
			"style": "powerline",
			"template": " îž² {{.Project}} :: {{.Account}}",
			"properties": {
				"display_default": true
			},
			"background": "transparent",
			"foreground": "#6666FF",
			"Text": "",
			"Duration": 0,
			"NameLength": 0
		}
	]
}
